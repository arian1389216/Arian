def dsa (message):
    div = {
        "a": "ุง", "b": "ุจ", "p": "ูพ", "t": "ุช", "s": "ุณ", "j": "ุฌ", "ch": "ฺ",
    "h": "ุญ", "kh": "ุฎ", "d": "ุฏ", "z": "ุฐ", "r": "ุฑ", "z": "ุฒ", "zh": "ฺ",
    "s": "ุณ", "sh": "ุด", "s": "ุต", "z": "ุถ", "t": "ุท", "z": "ุธ", "a": "ุน",
    "gh": "ุบ", "f": "ู", "gh": "ู", "k": "ฺฉ", "g": "ฺฏ", "l": "ู", "m": "ู",
    "n": "ู", "v": "ู", "h": "ู", "i": "", " " : "_"
    }
    b = ""
    a = message.text
    for i in a :
        d = div.get(i)
        b += str(d)
    bot.send_message(message.chat.id,b)

