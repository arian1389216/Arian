    bot.send_message(message.chat.id, "ุตุจุฑ ฺฉูุฏ ุฑุจุงุท ุฏุฑุญุงู ุฎูุงูุฏู ูพุงู ุดูุงุณุช")
    for i in words :
        for key , val in dic.items():
            if val == i :
                a += key 
                break
        a += "_"
    bot.send_message(message.chat.id, "ูุนู ูพุงู ุดูุง ุงู ุงุณุช:")
    bot.send_message(message.chat.id, a)

